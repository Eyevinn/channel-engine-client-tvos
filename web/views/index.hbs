<document>
<paradeTemplate>
   <list>
      <header>
         <title>Available Channels</title>
      </header>
      <section>
         {{#each channels as |channel|}}
         <listItemLockup onselect="play('{{channel.name}}'')">
            <title>{{channel.label}}</title>
         </listItemLockup>
         {{/each}}
      </section>
      <relatedContent>
         <imgDeck>
            <img src="http://localhost:8081/rick1.jpg" />
            <img src="http://localhost:8081/rick2.jpg" />
         </imgDeck>
      </relatedContent>
   </list>
</paradeTemplate>
</document>
